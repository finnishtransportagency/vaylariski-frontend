{
  "env": {
    "node": true,
    "browser": true,
    "es2021": true,
    "jest": true,
    "cypress/globals": true
  },
  "extends": ["eslint:recommended", "plugin:react/recommended", "prettier", "plugin:cypress/recommended"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "cypress"],
  "rules": {
    "react/react-in-jsx-scope": "off",
    "react/prop-types": "off",
    "linebreak-style": "off",
    "no-unused-vars": "warn",
    "quotes": "off"
  }
}
